.img-content {
  display: flex;
  align-items: center;
  justify-content: space-around;
  margin-bottom: 40rpx;
}
.logo1 {
  width: 37%;
  height: 146rpx;
}
.logo2 {
  width: 20%;
  height: 130rpx;
}